{
  "version": 1,
  "paths": {
    "/subscription": {
      "name": "/subscription",
      "lambdaFunction": "subscriptionHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create"
        ]
      }
    },
    "/payments": {
      "name": "/payments",
      "lambdaFunction": "paymentsHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}